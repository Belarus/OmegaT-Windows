gSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="250050">зворотний компенсатор кадрів</term></termGrp></ntig></langSet></termEntry><termEntry id="7681_262528"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A title animation in Windows Movie Maker.</descrip></descripGrp><ntig><termGrp><term id="262528">Fade, Wipe</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="325077">Затемнення, витирання</term></termGrp></ntig></langSet></termEntry><termEntry id="7691_156523"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An organization's requirements for complying with privacy regulations and directives.</descrip></descripGrp><ntig><termGrp><term id="156523">privacy policy</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1051898">політика конфіденційності</term></termGrp></ntig></langSet></termEntry><termEntry id="7698_56783"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The space available on a hard disk for creating primary partitions and extended partions, on which logical drives can be created, formatted, and assigned drive letters.</descrip></descripGrp><ntig><termGrp><term id="56783">free space</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="236031">вільне місце</term></termGrp></ntig></langSet></termEntry><termEntry id="7702_95380"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A set of instructions that a computer uses to perform a specific task, such as word processing, accounting, or data management.</descrip></descripGrp><ntig><termGrp><term id="95380">program</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244582">програма</term></termGrp></ntig></langSet></termEntry><termEntry id="7702_95381"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A set of instructions that a computer uses to perform a specific task, such as word processing, accounting, or data management.</descrip></descripGrp><ntig><termGrp><term id="95381">application</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1053794">застосунок</term></termGrp></ntig></langSet></termEntry><termEntry id="7706_36156"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An administrative security relationship between Simple Network Management Protocol () entities. For SNMP transactions, it is always necessary to specify the community name.</descrip></descripGrp><ntig><termGrp><term id="36156">community</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249229">спільнота</term></termGrp></ntig></langSet></termEntry><termEntry id="7712_130661"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An area of storage on a hard disk. A volume is formatted by using a file system, such as NTFS, and has a drive letter assigned to it. A single hard disk can have multiple volumes. Some volumes can span multiple hard disks.</descrip></descripGrp><ntig><termGrp><term id="130661">volume</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="235991">том</term></termGrp></ntig></langSet></termEntry><termEntry id="7719_175788"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A handle (often, a tall, thin rectangle) that appears on a rebar or command band. By touching and dragging a gripper bar with a stylus or mouse, a user can reposition or resize a rebar or command band.</descrip></descripGrp><ntig><termGrp><term id="175788">gripper bar</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="175811">захоплювальна смуга</term></termGrp></ntig></langSet></termEntry><termEntry id="7719_770797"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A handle (often, a tall, thin rectangle) that appears on a rebar or command band. By touching and dragging a gripper bar with a stylus or mouse, a user can reposition or resize a rebar or command band.</descrip></descripGrp><ntig><termGrp><term id="770797">gripper</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="781741">захват</term></termGrp></ntig></langSet></termEntry><termEntry id="7721_51155"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A processor or portion of a program that determines how the program manages and manipulates data. The term engine is most often used in relation to a specific use.</descrip></descripGrp><ntig><termGrp><term id="51155">engine</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249353">ядро</term></termGrp></ntig></langSet></termEntry><termEntry id="7723_262541"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">One of the music genres that appears under Genre classification in Windows Media Player library. Based on ID3 standard tagging format for MP3 audio files. ID3v1 genre ID # 30.</descrip></descripGrp><ntig><termGrp><term id="262541">Fusion</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="325090">Ф'южн</term></termGrp></ntig></langSet></termEntry><termEntry id="7725_175871"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A callback function that receives messages for a form or a control. Forms or controls that need to be notified of messages implement a message sink.</descrip></descripGrp><ntig><termGrp><term id="175871">message sink</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="175894">приймач повідомлень</term></termGrp></ntig></langSet></termEntry><termEntry id="7726_373353"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The act of following a link from one Web site to a site or page on another server when crawling for content.</descrip></descripGrp><ntig><termGrp><term id="373353">site hop</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="404845">перехід на інший сайт</term></termGrp></ntig></langSet></termEntry><termEntry id="7732_152509"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">To combine two interlaced fields into a single frame that is not interlaced.</descrip></descripGrp><ntig><termGrp><term id="152509">deinterlace</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="250116">перетворювати черезрядкову розгортку на прогресивну</term></termGrp></ntig></langSet></termEntry><termEntry id="7734_75813"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">In client/server network administration, the process of reducing heavy traffic flows either by dividing a busy network segment into multiple smaller segments or by using software to distribute traffic among multiple network interface cards working simultaneously to transfer information to a server.</descrip></descripGrp><ntig><termGrp><term id="75813">load balancing</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249508">розподілення навантаження</term></termGrp></ntig></langSet></termEntry><termEntry id="7735_317879"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A set of UI controls that appear in context when an object is selected and contain actions that act upon the object.</descrip></descripGrp><ntig><termGrp><term id="317879">Mini toolbar</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1086868">міні-панель</term></termGrp></ntig></langSet></termEntry><termEntry id="7741_176354"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An assembly language routine that does not call another routine.</descrip></descripGrp><ntig><termGrp><term id="176354">leaf routine</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="176376">кінцева програма</term></termGrp></ntig></langSet></termEntry><termEntry id="7745_173014"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The saved settings for file usage rights for the RM Services server in a specified domain. The administrator sets these in the Administration Console, and saves them for use by the client applications.</descrip></descripGrp><ntig><termGrp><term id="173014">rights template</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="173039">шаблон прав</term></termGrp></ntig></langSet></termEntry><termEntry id="7749_99659"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A group of related fields (columns) of information treated as a unit and arranged in a horizontal line in a table or spreadsheet.</descrip></descripGrp><ntig><termGrp><term id="99659">record</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1160405">запис</term></termGrp></ntig></langSet></termEntry><termEntry id="7753_42713"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A bar, area, dot, slice, or other symbol in a chart that represents a single data point or value.</descrip></descripGrp><ntig><termGrp><term id="42713">data marker</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="694370">позначка даних </term></termGrp></ntig></langSet></termEntry><termEntry id="7758_88413"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A type of field that contains a set of options.</descrip></descripGrp><ntig><termGrp><term id="88413">option set</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1375595">набір параметрів</term></termGrp></ntig></langSet></termEntry><termEntry id="7759_495127"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A presentation of information about a contact, including presence status and activity.</descrip></descripGrp><ntig><termGrp><term id="495127">contact card</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="503792">картка контакту</term></termGrp></ntig></langSet></termEntry><termEntry id="7761_156944"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A process that coordinates each user’s password changes across multiple computers, devices, folders, or networks in password synchronization.</descrip></descripGrp><ntig><termGrp><term id="156944">password propagation</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="250180">синхронізація паролів</term></termGrp></ntig></langSet></termEntry><termEntry id="7764_314130"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A type of hard disk that contains a small amount of solid-state nonvolatile memory which, when used as a cache, can realize significant benefits such as faster booting, extended battery life, and a more rugged PC. </descrip></descripGrp><ntig><termGrp><term id="314130">hybrid hard disk</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="373613">гібридний жорсткий диск</term></termGrp></ntig></langSet></termEntry><termEntry id="7770_323603"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A feature that enables Windows Vista PCs equipped with a hybrid hard disk drive to boot up faster, resume from hibernate in less time, and preserve battery power.</descrip></descripGrp><ntig><termGrp><term id="323603">Windows ReadyDrive</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="373544">Windows ReadyDrive</term></termGrp></ntig></langSet></termEntry><termEntry id="7774_233477"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The interval that is used to delay a response in a tarpitting scenario.</descrip></descripGrp><ntig><termGrp><term id="233477">tarpitting interval</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="254239">тривалість затримки</term></termGrp></ntig></langSet></termEntry><termEntry id="7785_120062"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The bar that contains the 'Start' button and appears by default at the bottom of the desktop. You can click the taskbar buttons to switch between programs. You can also hide the taskbar, move it to the sides or top of the desktop, and customize it in other ways.</descrip></descripGrp><ntig><termGrp><term id="120062">taskbar</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244625">панель завдань</term></termGrp></ntig></langSet></termEntry><termEntry id="7789_176832"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A recording format that can store audio, video, and pictures on a CD or DVD. HighMat discs play on any Windows computer and on some DVD players.</descrip></descripGrp><ntig><termGrp><term id="176832">High-Performance Media Access Technology</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="176879">технологія високошвидкісного доступу до носіїв</term></termGrp></ntig></langSet></termEntry><termEntry id="7789_176833"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A recording format that can store audio, video, and pictures on a CD or DVD. HighMat discs play on any Windows computer and on some DVD players.</descrip></descripGrp><ntig><termGrp><term id="176833">HighMAT</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="176878">технологія HighMAT</term></termGrp></ntig></langSet></termEntry><termEntry id="7793_265354"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A wizard that will help users to define a custom task for their workflow. Custom Task can be used to collect information from workflow participants. That information will be stored in the Tasks list for the site and can be used later in the workflow via workflow lookup.</descrip></descripGrp><ntig><termGrp><term id="265354">Custom Task Wizard</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="404546">майстер настроюваних задач</term></termGrp></ntig></langSet></termEntry><termEntry id="7794_234956"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A feature that allows the user to group search files by any criteria and display them in one place.</descrip></descripGrp><ntig><termGrp><term id="234956">Search Virtual Folder</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="260635">Пошук у віртуальній папці</term></termGrp></ntig></langSet></termEntry><termEntry id="7797_54308"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">In an operating system, the overall structure in which files are named, stored, and organized. NTFS, FAT, and FAT32 are types of file systems.</descrip></descripGrp><ntig><termGrp><term id="54308">file system</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244517">файлова система</term></termGrp></ntig></langSet></termEntry><termEntry id="7800_347938"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A folder in which a collection of forms based on the same template is stored and shared. Each form in a form library is associated with user-defined information that is displayed in the content listing for that library.</descrip></descripGrp><ntig><termGrp><term id="347938">form library</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="404675">бібліотека форм</term></termGrp></ntig></langSet></termEntry><termEntry id="7803_323609"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A service that is responsible for delivering kernel mode networking related notifications to user mode applications.</descrip></descripGrp><ntig><termGrp><term id="323609">Network Store Interface</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="422704">служба NSI</term></termGrp></ntig></langSet></termEntry><termEntry id="7804_159531"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A form of network security in which a remote access server calls a user back at a preset number after the user has made an initial connection and has been authenticated.</descrip></descripGrp><ntig><termGrp><term id="159531">callback security</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="159561">захист виклику</term></termGrp></ntig></langSet></termEntry><termEntry id="7805_62344"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A server computer that has access to other computers on the network. A host computer provides services such as news, mail, or data to computers that connect to it on a network.</descrip></descripGrp><ntig><termGrp><term id="62344">host computer</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249432">головний комп'ютер</term></termGrp></ntig></langSet></termEntry><termEntry id="7809_184963"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The full, legal version of product name, as it appears in Help/About dialog box.</descrip></descripGrp><ntig><termGrp><term id="184963">Microsoft Office Communicator 2005</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="345397">Microsoft Office Communicator 2005</term></termGrp></ntig></langSet></termEntry><termEntry id="7811_101213"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The default boot option used by most Windows DNS servers. When registry boot is used, DNS is started and initialized using DNS parameters and their values as they are stored in the Windows registry. You can use a Berkeley Internet Name Domain (BIND) boot file as an alternative to this method of boot configuration for DNS.</descrip></descripGrp><ntig><termGrp><term id="101213">registry boot</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249709">завантаження реєстру</term></termGrp></ntig></langSet></termEntry><termEntry id="7812_262672"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A title animation in Windows Movie Maker.</descrip></descripGrp><ntig><termGrp><term id="262672">Zoom, Out</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="325228">Зменшення</term></termGrp></ntig></langSet></termEntry><termEntry id="7813_203278"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A formal set of format, timing, sequencing, and error control rules for transferring files to and from a remote computer system running the Trivial File Transfer Protocol (TFTP) service.</descrip></descripGrp><ntig><termGrp><term id="203278">Trivial File Transfer Protocol</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249861">тривіальний протокол передавання файлів</term></termGrp></ntig></langSet></termEntry><termEntry id="7813_121188"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A formal set of format, timing, sequencing, and error control rules for transferring files to and from a remote computer system running the Trivial File Transfer Protocol (TFTP) service.</descrip></descripGrp><ntig><termGrp><term id="121188">TFTP</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249859">TFTP</term></termGrp></ntig></langSet></termEntry><termEntry id="7813_121189"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A formal set of format, timing, sequencing, and error control rules for transferring files to and from a remote computer system running the Trivial File Transfer Protocol (TFTP) service.</descrip></descripGrp><ntig><termGrp><term id="121189">Trivial FTP</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249860">тривіальний FTP</term></termGrp></ntig></langSet></termEntry><termEntry id="7814_347948"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An XML data file, database, or Web service that is used by a form for the entries in a list box or for script actions.</descrip></descripGrp><ntig><termGrp><term id="347948">secondary data source</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="404689">додаткове джерело даних</term></termGrp></ntig></langSet></termEntry><termEntry id="7820_145532"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An application or DLL that supplies a linked or embedded OLE object to another application. For example, if an OLE object in an Access database contains a Microsoft Excel worksheet, Excel is the OLE server.</descrip></descripGrp><ntig><termGrp><term id="145532">OLE server</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="694484">сервер OLE</term></termGrp></ntig></langSet></termEntry><termEntry id="7822_177676"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The right pane in DPM Administrator Console. The Actions pane lists the operations that an administrator can perform within a selected task area.</descrip></descripGrp><ntig><termGrp><term id="177676">Actions pane</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="177703">панель дій</term></termGrp></ntig></langSet></termEntry><termEntry id="7823_126732"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The password stored in each user's account. Each user generally has a unique user password and must type that password when logging on or accessing a server.</descrip></descripGrp><ntig><termGrp><term id="126732">user password</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244641">пароль користувача</term></termGrp></ntig></langSet></termEntry><termEntry id="7824_184495"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An item on the Actions menu that calls the person the user selects from the corporate address book.</descrip></descripGrp><ntig><termGrp><term id="184495">Call Someone...</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="345345">Викликати абонента…</term></termGrp></ntig></langSet></termEntry><termEntry id="7826_211426"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A driver that is not included with Windows.</descrip></descripGrp><ntig><termGrp><term id="211426">out-of-box driver</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244735">драйвер від постачальника обладнання</term></termGrp></ntig></langSet></termEntry><termEntry id="7827_16956"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A user access option that you specify at the default Web site level or Application level in IIS. Authenticated access requires a user to have a valid account and password to access the site.</descrip></descripGrp><ntig><termGrp><term id="16956">authenticated access</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249004">доступ з автентифікацією</term></termGrp></ntig></langSet></termEntry><termEntry id="7828_5053"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The particular algorithm used to transform a readable message (called plaintext or cleartext) into an unreadable, scrambled, or hidden message (called ciphertext).</descrip></descripGrp><ntig><termGrp><term id="5053">cipher</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="248840">шифр</term></termGrp></ntig></langSet></termEntry><termEntry id="7829_185376"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An item on the Text Size menu, which is opened from the Edit menu. Smallest is one of the text size choices for the user's instant messages</descrip></descripGrp><ntig><termGrp><term id="185376">Smallest</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="345443">Найменший</term></termGrp></ntig></langSet></termEntry><termEntry id="7830_269816"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An application designed for managing Microsoft Windows Mobile® devices via a Windows Vista client computer. When the mobile device is connected to the computer by cable, cradle, infrared, or wireless LAN, the mobile user uses Windows Mobile Device Center to create a partnership between the desktop computer and the mobile device to configure the device, manage settings, add &amp; remove applications, and synchronize data. After the partnership is created, the mobile user can manage the device using any of the connection mechanisms. While connected, the mobile user can also use the Windows Vista client computer without a partnership to do such things as browse the device and configure settings for the device.</descrip></descripGrp><ntig><termGrp><term id="269816">Windows Mobile Device Center</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1356774">центр пристроїв Windows Mobile</term></termGrp></ntig></langSet></termEntry><termEntry id="7831_151832"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The process of lowering the frame rate of source video to reduce the bandwidth required for streaming to be lower than or equal to the available client bandwidth. If necessary, the video portion of the stream may stop streaming and only the audio portion streamed.</descrip></descripGrp><ntig><termGrp><term id="151832">stream thinning</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="250090">проріджування потоку</term></termGrp></ntig></langSet></termEntry><termEntry id="7834_358352"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A service that returns server configuration information for a mobile device or other client using only the user's SMTP e-mail address and password.</descrip></descripGrp><ntig><termGrp><term id="358352">Autodiscover service</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="363149">Служба автовизначення конфігурації сервера</term></termGrp></ntig></langSet></termEntry><termEntry id="7836_160450"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A type of plug-in that enables a Windows Media server to communicate with clients and other servers through different network protocols.</descrip></descripGrp><ntig><termGrp><term id="160450">control protocol plug-in</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="160479">компонент plug-in протоколу керування</term></termGrp></ntig></langSet></termEntry><termEntry id="7837_353903"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A set of records defined by a table, a query, or an SQL expression.</descrip></descripGrp><ntig><termGrp><term id="353903">domain</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="399138">домен</term></termGrp></ntig></langSet></termEntry><termEntry id="7838_213840"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An input device consisting of a set of individual keys similar to those on a typewriter. It is used to convey information from a user to a computer or data communications circuit.</descrip></descripGrp><ntig><termGrp><term id="213840">standard keyboard</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="239052">стандартна клавіатура</term></termGrp></ntig></langSet></termEntry><termEntry id="7838_72879"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">An input device consisting of a set of individual keys similar to those on a typewriter. It is used to convey information from a user to a computer or data communications circuit.</descrip></descripGrp><ntig><termGrp><term id="72879">keyboard</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249492">клавіатура</term></termGrp></ntig></langSet></termEntry><termEntry id="7839_178240"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">The person who sets up and maintains the systems and records related to authorized users of multi-user computer networks or operating systems.</descrip></descripGrp><ntig><termGrp><term id="178240">account manager</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="250196">диспетчер облікових записів</term></termGrp></ntig></langSet></termEntry><termEntry id="7840_130989"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A processing cycle of the microprocessor during which it only waits for data from an input/output device or from memory. While a single wait state is not humanly perceptible, the cumulative effect of wait states is to slow system performance.</descrip></descripGrp><ntig><termGrp><term id="130989">wait state</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249927">стан очікування</term></termGrp></ntig></langSet></termEntry><termEntry id="7841_12156"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A record of synthetic, successive still images that produce an illusion of movement when played back.</descrip></descripGrp><ntig><termGrp><term id="12156">animation</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="1138268">анімація</term></termGrp></ntig></langSet></termEntry><termEntry id="7843_211411"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A mode of running Sysprep that postpones the first-run experience and enables OEM and corporations to install additional drivers and applications, incorporate build-to-order customer information, and test the Windows installation. To run Sysprep in factory mode, use the command line b&amp;gt;Sysprep -factory/b&amp;gt;.</descrip></descripGrp><ntig><termGrp><term id="211411">factory mode</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="244720">заводський режим</term></termGrp></ntig></langSet></termEntry><termEntry id="7852_45965"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">Pertaining to the state of a control in which normal functionality is not available to a user.</descrip></descripGrp><ntig><termGrp><term id="45965">dimmed</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249305">"сіра"</term></termGrp></ntig></langSet></termEntry><termEntry id="7852_45966"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">Pertaining to the state of a control in which normal functionality is not available to a user.</descrip></descripGrp><ntig><termGrp><term id="45966">unavailable</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="249306">недоступний</term></termGrp></ntig></langSet></termEntry><termEntry id="7853_184738"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">In the Add a Contact Wizard, one of the fields by which you can search for a contact.</descrip></descripGrp><ntig><termGrp><term id="184738">First Name</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="345371">Ім'я</term></termGrp></ntig></langSet></termEntry><termEntry id="7854_184016"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A label that refers to the e-mail address of the person responsible for editorial content.</descrip></descripGrp><ntig><termGrp><term id="184016">managing editor</term></termGrp></ntig></langSet><langSet xml:lang="uk-ua"><ntig><termGrp><term id="404464">головний редактор</term></termGrp></ntig></langSet></termEntry><termEntry id="7861_212222"><langSet xml:lang="en-US"><descripGrp><descrip type="definition">A file created for a particular adapter that provides the operating system with information required to set up a device, such as a list of valid logical configurations for the device, the names of driver files associated with the device, and so on